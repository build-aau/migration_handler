{
  "name": "build-script-code",
  "displayName": "build_script_code",
  "description": "highlighting of build script",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bs",
      "aliases": ["BUILD script", "bs"],
      "extensions": [".bs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bs",
      "scopeName": "source.bs",
      "path": "./syntaxes/bs.tmLanguage.json"
    }]
  }
}
